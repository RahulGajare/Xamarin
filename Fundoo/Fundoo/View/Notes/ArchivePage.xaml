<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Fundoo.View.ArchivePage">

  

    <ContentPage.Content>
        <StackLayout>
            <StackLayout>
                <StackLayout Margin="7,0,7,0">
                    <Label x:Name="pinnedLabel" Text ="Pinned Notes" IsVisible="False" FontAttributes="Bold"/>
                    <Grid x:Name="gridLayoutPinned">

                    </Grid>

                </StackLayout>

                <StackLayout Margin="7,0,7,0">
                    <Label x:Name="UnpinnedLabel" Text="Others" IsVisible="False" FontAttributes="Bold"/>
                    <Grid x:Name="gridLayoutUnpinned">

                    </Grid>

                </StackLayout>
            </StackLayout>
            </StackLayout>
    </ContentPage.Content>
</ContentPage>