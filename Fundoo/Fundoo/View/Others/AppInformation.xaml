<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Fundoo.View.Others.AppInformation">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical" VerticalOptions="Start">
            <Label x:Name="Name" FontSize="Large"/>
            <Label x:Name="PackageName" FontSize="Large"/>
            <Label x:Name="VersionString" FontSize="Large"/>
            <Label x:Name="BuildString" FontSize="Large"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>